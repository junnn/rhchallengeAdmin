<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:c="urn:import:com.google.gwt.user.cellview.client">

    <g:HTMLPanel>
        <h1>Search Page</h1>
        <c:CellTable ui:field="cellTable"/>
        <g:TextBox ui:field="searchTextBox"/><br/>
        <g:Button ui:field="searchButton" text="search"/>
        <g:ListBox ui:field="fieldListBox" visibleItemCount="1">
            <g:item value="1">Email</g:item>
            <g:item value="2">Country</g:item>
        </g:ListBox><br/>
        <g:ListBox ui:field="resultListBox"/><br/>
    </g:HTMLPanel>
</ui:UiBinder>
